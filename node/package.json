{
  "name": "@fpjs-incubator/broyster",
  "description": "Unified test tools (Vitest + WebdriverIO + BrowserStack)",
  "version": "1.0.0",
  "bin": {
    "broyster": "./bin/broyster.js"
  },
  "bugs": "https://github.com/fingerprintjs/broyster/issues",
  "dependencies": {
    "@wdio/browserstack-service": "^9.18.4",
    "@wdio/cli": "^9.18.4",
    "@wdio/local-runner": "^9.18.4",
    "@wdio/mocha-framework": "^9.18.0",
    "@wdio/spec-reporter": "^9.18.0",
    "tslib": "^2.8.1",
    "webdriverio": "^9.18.4"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.2.0",
    "normalize-package-data": "^8.0.0",
    "puppeteer-core": "^24.15.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./wdio": {
      "types": "./dist/wdio/config.d.ts",
      "import": "./dist/wdio/config.js"
    },
    "./vitest": {
      "types": "./dist/vitest/preset.d.ts",
      "import": "./dist/vitest/preset.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/fingerprintjs/broyster",
  "keywords": [
    "browserstack",
    "test",
    "tools",
    "vitest",
    "webdriverio"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "peerDependencies": {
    "vitest": "^3.2.4"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "repository": "fingerprintjs/broyster.git",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "prepack": "cp ../LICENSE LICENSE"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}
